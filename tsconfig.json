{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "es6",
        "target": "es5",
        "sourceMap": true,
        "rootDir": "src",
        "outDir": "dist",
        "jsx": "react",
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "baseUrl": ".",
        "paths": {
            "components/*": ["./src/components/*"],
            "assets/*": ["./src/assets/*"]
        }
    },
    "exclude": [
        "node_modules",
        "out",
        "dangerfile.js",
        "data",
        "externals"
    ]
}
